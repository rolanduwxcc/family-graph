{
  "name": "family-graph",
  "version": "1.0.0",
  "description": "Family Graph Application",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolanduwxcc/family-graph.git"
  },
  "keywords": [],
  "author": "BMW",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rolanduwxcc/family-graph/issues"
  },
  "homepage": "https://github.com/rolanduwxcc/family-graph#readme",
  "devDependencies": {
    "concurrently": "^6.1.0",
    "flow-bin": "^0.152.0"
  },
  "dependencies": {
    "@microstates/react": "^0.12.0",
    "flowpoints": "^1.0.18",
    "if-env": "^1.0.4",
    "jest": "^26.6.0",
    "microstates": "^0.12.4",
    "react-dom": "^16.14.0",
    "react-family-tree": "^3.0.0",
    "react-flow-renderer": "^9.5.4",
    "react-scripts": "^4.0.3"
  }
}
